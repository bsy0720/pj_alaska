<div class="sub_visual">
    <div class="bg orora">
        <p class="title">고객센터</p>
        <p class="txt">내가 꿈꾸던 여행지를 현실에서 만나세요!</p>
    </div>
</div>
<div class="w1200 content_box">
    <div id="content">
        <div class="subpg_title_wrap">
            <h2 class="subpg_title">여행상품 Q&A</h2>
        </div>
        <div class="main_t_wrap">
            <a href="./alaska_Q&A_write">글쓰기</a>
        </div>

        <div class="q_table_wrap">
            <div class="n_table">
                <table border="1">
                    <colgroup>
                        <col style="width:100px">
                        <col style="width:auto">
                        <col style="width:100px">
                        <col style="width:150px">
                    </colgroup>
                    <thead>
                        <tr>
                            <th scope="col">번호</th>
                            <th scope="col">제목</th>
                            <th scope="col">글쓴이</th>
                            <th scope="col">작성일</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% rows.forEach((row, index) => { %>
                        <tr>
                            <td class="center imp"><%=row.RNUM%></td>
                            <td>
                                <a class="accordion"><%=row.title%></a>
                                <div class="panel">
                                    <div class="pa">
                                        <p><%=row.content%></p>
                                    </div>
                                    <div id="b_btn">
                                        <input type="button"
                                            onclick="javascript:location.href = '/newQna?id=<%=row.id%>'" value="수정">
                                        <input type="button"
                                            onclick="javascript:location.href = '/deleteQna?id=<%=row.id%>'" value="삭제">
                                        </input>
                                    </div>
                            </td>
                            <td class="center"><%= row.writer %></td>
                            <td class="center"><%= row.created %></td>
                        </tr>
                        <% }) %>

                    </tbody>
                </table>
            </div>
        </div>
        <div class="mb_n_table_wrap">
            <div class="mb_n_table">
                <table border="1">
                    <colgroup>
                        <col style="width:100px">
                        <col style="width:auto">
                        <col style="width:150px">
                    </colgroup>
                    <thead>
                        <tr>
                            <th scope="col">번호</th>
                            <th scope="col">제목</th>
                            <th scope="col">작성일</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% rows.forEach((row, index) => { %>
                        <tr>
                            <td class="center imp"><%= row.RNUM %></td>
                            <td>
                                <a class="accordion"><%=row.title%></a>
                                <div class="panel">
                                    <p><%=row.content%></p>
                                </div>
                            </td>
                            <td class="center"><%= row.created %></td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>


        <div class="m_b_number">
            <a href="#" class="n_left"></a>
            <a href="#" class="b_number">1</a>
            <a href="#" class="n_right"></a>
        </div>
    </div>
</div>

<a href="#" class="top">
    <img src="../img\logo\b_top.png">
</a>
<div class="side_quick_menu">
    <div class="side_container">
        <div class="side_currency">
            <select id="side_currency-one">
                <option value="KRW" selected>KRW</option>
                <option value="USD">USD</option>
            </select>
            <input type="number" id="side_amount-one" placeholder="0" value="1">
        </div>
        <div class="side_swap-rate-container">
            <button class="side_btn" id="side_swap">⇅</button>
        </div>
        <div class="side_currency">
            <select id="side_currency-two">
                <option value="KRW">KRW</option>
                <option value="USD" selected>USD</option>
            </select>
            <input type="number" id="side_amount-two" placeholder="0">
        </div>
    </div>
    <div class="side_rate" id="side_rate"></div>
    <div class="side_weather">
        <div class="l_box">
            <div class="s_CurrIcon"></div>
        </div>
        <div class="r_box">
            <div class="s_CurrTemp"></div>
            <div class="s_city"></div>
        </div>
    </div>
</div>